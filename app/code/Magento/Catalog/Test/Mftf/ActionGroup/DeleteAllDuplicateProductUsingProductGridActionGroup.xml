<actionGroups xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/actionGroupSchema.xsd">
    <actionGroup name="DeleteAllDuplicateProductUsingProductGridActionGroup" extends="DeleteProductUsingProductGridActionGroup">
        <annotations>
            <description>EXTENDS: DeleteProductUsingProductGridActionGroup. Removes 'seeProductSkuInGrid'.</description>
        </annotations>
        <arguments>
            <argument name="product"/>
        </arguments>

        <remove keyForRemoval="seeProductSkuInGrid"/>
    </actionGroup>
</actionGroups>
